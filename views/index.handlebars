  <div id="index-banner" class="parallax-container">
    <div class="section no-pad-bot">
      <div class="container">
        <br><br>
        <h1 class="header center teal-text text-lighten-2">Let's Eat Some Burgers</h1>
        <div class="row center">
          <h5 class="header col s12 light">A Nodejs Express app that uses handlebars and mySQL to dynamically update information on page.</h5>
        </div>
        <div class="row center">
          <a href="#center-block" id="download-button" class="btn-large waves-effect waves-light teal lighten-1">Get Started</a>
        </div>
        <br><br>

      </div>
    </div>
    <div class="parallax"><img src="burger1.jpg" alt="Unsplashed background img 1"></div>
  </div>


  <div class="container">
    <div class="section">
      <div class="row" id="center-block">
      	<div class="container">
			<div class="row">
				<div class="col-xs-6 text-center" class="task">
					<div class="row">
						{{#each burger_data}}
							{{#unless this.devoured}}
							<div class="col-xs-9 text-center">
								<pre>{{this.id}}. {{this.burger_name}}</pre>
							</div>
						<div class="col-xs-3 text-center">
							<form method="POST" action="/burgers/update?_method=PUT" class="button-size">
								<input type="hidden" name="burger_id" type="text" value={{this.id}}>
								<br>
								<button type="submit" class="btn btn-default">Time for heartburn</button>
							</form>
						</div>
						{{/unless}}
						{{/each}}
					</div>
				</div>
				
				<div class="col-xs-6 text-center" class="task">
					{{#each burger_data}}
						{{#if this.devoured}}
							<input type="text" class="form-control" placeholder="{{this.id}}. {{this.burger_name}}" readonly>
						{{/if}}
					{{/each}}
				</div>

			</div>
		</div>
      </div>
    </div>
  </div>


  <div class="parallax-container valign-wrapper">
    <div class="section no-pad-bot">
      <div class="container">
        <div class="row center">
          <h5 class="header col s12 light">In the mood for a different type of burger?</h5>
        </div>
      </div>
    </div>
    <div class="parallax"><img src="burger2.jpg" alt="Unsplashed background img 2"></div>
  </div>

  <div class="container">
    <div class="section">
	    	<div class="container">
				<div class="row">
					<div class="col-xs-12 text-center enter_section">
					<form action="/burgers/create" method="POST">
						<input type="text" name="burger_name" class="form-control" value="" id="enter_text">
						<button type="submit" class="btn btn-primary" id="text-enter-button">SUBMIT</button>
					</form>
					<p>Enter a burger you'd like to eat</p>
					</div>
				</div>
			</div>
    </div>
  </div>


  <div class="parallax-container valign-wrapper">
    <div class="section no-pad-bot">
      <div class="container">
        <div class="row center">
          <h5 class="header col s12 light">Don't forget to pair your burger with some refreshing beer</h5>
        </div>
      </div>
    </div>
    <div class="parallax"><img src="beer.gif" alt="Unsplashed background img 3"></div>
  </div>

  <footer class="page-footer teal">
    <div class="footer-copyright">
      <div class="container">
      Made by <a class="brown-text text-lighten-3" href="https://mapipina.github.io/portfolio/">Mapi Pina</a>
      </div>
    </div>
  </footer>
